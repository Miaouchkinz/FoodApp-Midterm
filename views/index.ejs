<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Meta Information -->
    <title>Food App</title>

    <!-- External CSS -->
    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
    <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
    <link rel="stylesheet" href="/styles/layout.css" type="text/css" />
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Bungee|Righteous|Source+Sans+Pro&display=swap" rel="stylesheet">
    <script src="https://kit.fontawesome.com/6ba3f5c144.js" crossorigin="anonymous"></script>

    <!-- Vendor JS -->
    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
    <script type="text/javascript" src="/scripts/app.js"></script>

    <!-- App JS -->
    <script type="text/javascript" src="/scripts/menuCategory.js"></script>


    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css" integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS" crossorigin="anonymous">

  </head>

  <body>
  <header>
    <nav>

      <div></div>

      <div class="restaurant-name">
        <img src="/images/dragon12.png" alt="dragon">
        <span class="space-between"></span>
        <h2>Camilo<span>'s</span> Cafe</h2>
      </div>

        <img src="/images/cart7.png" alt="Cart">
    </nav>
  </header>
  <main>

    <!-- menu category container -->

    <section class="menu-category">
      <div class="conainer-lg">
        <div class="row">

          <div class="col-lg-3 sandwich" id="sandwich">
            <img src="/images/sandwich7.png" alt="sandwich">
            <p>sandwich</p>
          </div>
          <div class="col-lg-3" id="soup">
            <img src="/images/soup2.png" alt="soup">
            <p>soup</p>
          </div>
          <div class="col-lg-3" id="salad">
            <img src="/images/salad2.png" alt="salad">
            <p>salad</p>
          </div>
          <div class="col-lg-3" id="drinks">
            <img src="/images/drink2.png" alt="drink">
            <p>drink</p>
          </div>

        </div>
      </div>
    </section>

    <!-- meal-items-containers -->

    <section id="meal-items">

    </section>

    <!-- place-order-container -->
    <section class="place-order">
        <div class="conainer-lg">
            <div class="row place-order-row">
                <div class="col-lg-12">
                    <div class="conainer-lg">
                      <div class="row nested-place-order-row">

                          <div class="col-lg-7 total-price">
                            <div class="conainer-lg">
                            </div>
                            <p>
                              Total = $62.40
                            </p>
                          </div>
                          <div class="col-lg-2">
                              <p>

                              </p>
                          </div>
                          <div class="col-lg-3 place-order">
                              <p>
                              place order
                              </p>
                          </div>
                      </div>

                      <div class="row nested-tip-row">
                          <div class="col-lg-1 tip">10%</div>
                          <div class="col-lg-1 tip">15%</div>
                          <div class="col-lg-1 tip">20%</div>
                          <div class="col-lg-1 tip">25%</div>
                          <div class="col-lg-2 tip">place order</div>
                          <div class="col-lg-5"></div>

                      </div>

                    </div>
                </div>
            </div>
        </div>

    </section>

    <!-- new-order -->
    <section class="new-order">
      <div class="conainer-lg">
        <div class="row new-order-row">
          <div class="col-lg-1 bell my-auto">
            <img src="/images/bell2.png" alt="bell">
          </div>
          <div class="col-lg-1 avatar">
            <p>New Order #</p>
            <img src="/images/user2.png" alt="user">
          </div>
          <div class="col-lg-1 my-auto"></div>
          <div class="col-lg-6 order-list my-auto">order-list</div>
          <div class="col-lg-1 my-auto"></div>
          <div class="col-lg-2 decision-sms my-auto">
            <div>
              <img src="/images/check-mark3.png" alt="check-mark">
              <img src="/images/x-mark3.png" alt="x-mark">
            </div>
            <br>
            <div class="sms">
              sms
            </div>
          </div>

        </div>
      </div>
    </section>

    <!-- pick-up -->
    <section class="pick-up">
      <div class="conainer-lg">
        <div class="row pick-up-row">
          <div class="col-lg-1"></div>
          <div class="col-lg-4 text-left">
            <p>
              <h2>COMPLTETED</h2>
            </p>
            <p>
              <h2>READY FOR PICK UP</h2>
            </p>
          </div>
          <div class="col-lg-2"></div>
          <div class="col"></div>
          <div class="w-100"></div>
          <div class="col-lg-1 my-auto"></div>
          <div class="col-lg-2 my-auto">
            <img src="/images/user-circle3.png" alt="user-circle3"><br>
            <p>
              new-order #
            </p>
          </div>
          <div class="col-lg-2 my-auto">
            <img src="/images/user-circle3.png" alt="user-circle3"><br>
            <p>
              new-order #
            </p>
          </div>
          <div class="col-lg-2 my-auto">
            <img src="/images/user-circle3.png" alt="user-circle3"><br>
            <p>
              new-order #
            </p>
          </div>
          <div class="col-lg-2 my-auto">
            <img src="/images/user-circle3.png" alt="user-circle3"><br>
            <p>
              new-order #
            </p>
          </div>
          <div class="col-lg-2 my-auto">
            <img src="/images/user-circle3.png" alt="user-circle3"><br>
            <p>
              new-order #
            </p>
          </div>
          <div class="col-lg-1 my-auto"></div>
        </div>
      </div>
    </section>





  </main>

  <!-- Bootstrap JS -->
